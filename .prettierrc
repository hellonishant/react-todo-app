{
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": false,
        "requirePragma": false,
        "proseWrap": "preserve",
        "arrowParens": "avoid",
        "htmlWhitespaceSensitivity": "css",
        "quoteProps": "as-needed",
        "trailingComma": "es5",
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "semi": true,
        "useTabs": true,
        "bracketSpacing": true,
        "endOfLine": "lf"
}